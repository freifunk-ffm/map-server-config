{
  "dataPath": [
    "/hopglass/"
  ],
  "siteName": "Freifunk Frankfurt",
  "mapSigmaScale": 0.5,
  "showContact": true,
  "maxAge": 14,
  "mapLayers": [
    { "name": "MapQuestOpen.OSM" },
    { "name": "OpenStreetMap.HOT" },
    { "name": "OpenStreetMap.Mapnik" },
    { "name": "OpenStreetMap.DE" },
    { "name": "Stamen.TonerLite" },
    { "name": "Esri.WorldImagery" }
  ],
  "siteNames": [
    { "site": "ffffm", "name": "Freifunk Frankfurt" }
  ],
  "hwImg": [
    { "thumbnail": "https://cdn.rawgit.com/Moorviper/meshviewer_hwpics/master/nodes/{MODELHASH}.svg", "caption": "Knoten {MODELHASH}" }
  ],
  "nodeInfos": [
    { "iframe": "/grafana/dashboard-solo/db/node?panelId=5&theme=light&var-node={NODE_ID}", "height": "250px" },
    { "iframe": "/grafana/dashboard-solo/db/node?panelId=3&theme=light&var-node={NODE_ID}", "height": "250px" },
    { "iframe": "/grafana/dashboard-solo/db/node?panelId=4&theme=light&var-node={NODE_ID}", "height": "250px" },
    { "iframe": "/grafana/dashboard-solo/db/node?panelId=1&theme=light&var-node={NODE_ID}", "height": "200px" },
    { "iframe": "/grafana/dashboard-solo/db/node?panelId=2&theme=light&var-node={NODE_ID}", "height": "400px" }
  ],
  "linkInfos": [
    { "iframe": "/grafana/dashboard-solo/db/link?panelId=1&theme=light&var-node={SOURCE}&var-node={TARGET}&var-source={SOURCE}&var-target={TARGET}", "height": "300px" },
    { "iframe": "/grafana/dashboard-solo/db/link?panelId=3&theme=light&var-node={SOURCE}&var-node={TARGET}&var-source={SOURCE}&var-target={TARGET}", "height": "300px" },
    { "iframe": "/grafana/dashboard-solo/db/link?panelId=4&theme=light&var-node={SOURCE}&var-node={TARGET}&var-source={SOURCE}&var-target={TARGET}", "height": "300px" }
  ], 
  "globalInfos": [
    { "iframe": "/grafana/dashboard-solo/db/global?panelId=3&theme=light", "height": "300px" },
    { "iframe": "/grafana/dashboard-solo/db/global?panelId=4&theme=light", "height": "300px" },
    { "iframe": "/grafana/dashboard-solo/db/global?panelId=8&theme=light", "height": "400px" },
    { "iframe": "/grafana/dashboard-solo/db/global?panelId=9&theme=light", "height": "400px" },
    { "iframe": "/grafana/dashboard-solo/db/global?panelId=10&theme=light", "height": "400px" },
    { "iframe": "/grafana/dashboard-solo/db/extras?panelId=11&theme=light", "height": "250px" },
    { "caption": "WifiAnalyzer_Alias.txt", "href": "/hopglass/wifi-aliases.txt" },
    { "caption": "HOSTS-Datei", "href": "/hopglass/hosts" }
  ]  
